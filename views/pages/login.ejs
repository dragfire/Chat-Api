<!doctype html>
<html>
<head>
    <% include ../partials/head %>
</head>
<body>
<div class="col s12 m12 l12">
    <div class="row">
        <div class="col m3 l3">
            <div class="input-field">
            </div>
        </div>
        <form class="col s12 m6 l6" method="post" onsubmit="if(this.email.value == '' || this.password.value == '') return false; return true;">
            <div class="msg-board center center-align">

            </div>
            <div class="input-field">
                <i class="material-icons prefix">email</i>
                <input id="email" type="text" class="validate" name="email">
                <label for="email">Email</label>
            </div>
            <div class="input-field">
                <i class="material-icons prefix">lock</i>
                <input id="password" type="password" class="validate" name="password">
                <label for="password">Password</label>
            </div>
            <button class="waves-effect waves-light btn-large" type="submit"><i class="material-icons right">send</i>Login
            </button>
        </form>
    </div>
</div>
</body>
